{
  "exclude": [
    "node_modules",
    "types",
    "scripts",
    "build"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true
  },
  "lib": [
    "dom",
    "scripthost",
    "es5",
    "es2015.promise",
  ]
}
